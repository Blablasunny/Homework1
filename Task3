package hw1;

import java.util.Scanner;
public class Task3 {
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        int k= in.nextInt();
        int start = in.nextInt();
        SumOfNums(k, start);
    }
    public static void SumOfNums(int k, int start)
    {
        System.out.print(start);
        for (int i = 1; i < k; i++){System.out.print(" + " + (start + 10 * i));}
    }
}
